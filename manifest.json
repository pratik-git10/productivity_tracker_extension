{
  "manifest_version": 3,
  "name": "Productivity Time Tracker",
  "description": "Tracks time spent on different websites.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "activeTab"]
}
